{
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@/components/*": ["components/*"],
			"@/layouts/*": ["layouts/*"],
			"@/partials/*": ["partials/*"],
			"@/styles/*": ["styles/*"],
			"@/models/*": ["models/*"]
		},
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/models/Contents.ts"],
	"exclude": ["node_modules"]
}
